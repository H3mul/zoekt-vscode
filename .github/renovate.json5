{
    $schema: 'https://docs.renovatebot.com/renovate-schema.json',
    extends: [
        'config:best-practices',
        'helpers:pinGitHubActionDigests',
    ],
    onboarding: false,
    requireConfig: 'ignored',
    platform: 'github',
    repositories: [
        'h3mul/zoekt-vscode',
    ],
    prConcurrentLimit: 0,
    prHourlyLimit: 0,
    branchConcurrentLimit: 0,
    automerge: true,
    labels: [
        'renovate',
    ],
    rebaseWhen: 'behind-base-branch',
    packageRules: [
        {
            semanticCommitType: 'chore',
            matchPackageNames: [
                '*',
            ],
        },
        {
            groupName: 'all patch minor and digest',
            matchPackageNames: [
                '*',
            ],
            matchUpdateTypes: [
                'patch',
                'minor',
                'digest',
            ],
            automerge: true,
        },
    ],
}
